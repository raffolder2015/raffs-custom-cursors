; mousewritten-alpha

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; -- Common Information

[Scheme.Cur]
normal.cur
uhhhwhatthehell.cur
background.cur
busy.cur
text.cur
unavailable.cur
resizeV.cur
resizeH.cur
resizeHV1.cur
resizeHV2.cur
move.cur
hyperlink.cur
precision.cur
pen_rm.cur
alternate.cur
[Strings]
CUR_DIR       = "Cursors\mousewritten-alpha"
SCHEME_NAME   = "mousewritten-alpha"
pointer       = "normal.cur"
help          = "uhhhwhatthehell.cur"
work          = "background.cur"
busy          = "busy.cur"
text          = "text.cur"
unavailiable  = "unavailable.cur"
vert          = "resizeH.cur"
horz          = "resizeV.cur"
dgn1          = "resizeHV1.cur"
dgn2          = "resizeHV2.cur"
move          = "move.cur"
link          = "hyperlink.cur"
cross         = "precision.cur"
hand          = "pen_rm.cur"
alternate     = "alternate.cur"
